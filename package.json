{
  "name": "nestjs-discord",
  "version": "1.0.0",
  "description": "A Discord.js client provider for NestJS",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --verbose",
    "prebuild": "barrelsby -c barrelsby.json && rm -rf ./dist",
    "build": "tsc --p tsconfig.json",
    "build:watch": "tsc -w"
  },
  "dependencies": {
    "discord.js": "^12.3.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/testing": "^7.4.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "^4.2.0",
    "barrelsby": "^2.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "rxjs": "^6.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "discord.js",
    "discord",
    "nestjs",
    "nest.js"
  ],
  "author": "BitForger (https://github.com/BitForger)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitForger/nestjs-discord"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitForger/nestjs-discord.git"
  }
}
