{
  "name": "nestjs-discord",
  "version": "1.0.0",
  "description": "A Discord.js client provider for NestJS",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --verbose",
    "prebuild": "barrelsby -c barrelsby.json && rm -rf ./dist",
    "build": "tsc --p tsconfig.json",
    "build:watch": "tsc -w"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "discord.js": "^13.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "barrelsby": "^2.3.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "rxjs": "^7.4.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "discord.js",
    "discord",
    "nestjs",
    "nest.js"
  ],
  "author": "BitForger (https://github.com/BitForger)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitForger/nestjs-discord"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitForger/nestjs-discord.git"
  }
}
